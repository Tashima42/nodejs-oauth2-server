{
  "name": "oauth2-poc",
  "version": "1.0.0",
  "description": "An example of how oauth works",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon --exec ts-node --transpile-only src/index.ts",
    "build": "tsc --build",
    "db:migrate": "ts-node --transpile-only scripts/database.ts --migrate",
    "db:populate": "ts-node --transpile-only scripts/database.ts --populate",
    "db:migrate:populate": "ts-node --transpile-only scripts/database.ts --migrate --populate"
  },
  "author": "tashima42",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "sqlite": "^4.0.23",
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/sqlite3": "^3.1.8",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
